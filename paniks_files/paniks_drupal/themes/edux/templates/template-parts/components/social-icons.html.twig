<ul class="social-icons">
  {% if facebook_url != '' %}
    <li><a href="{{ facebook_url }}" target="_blank"><i class="icofont-facebook"></i></a></li>
  {% endif %}
  {% if twitter_url != '' %}
    <li><a href="{{ twitter_url }}" target="_blank"><i class="icofont-twitter"></i></a></li>
  {% endif %}
  {% if instagram_url != '' %}
    <li><a href="{{ instagram_url }}" target="_blank"><i class="icofont-instagram"></i></a></li>
  {% endif %}
  {% if linkedin_url != '' %}
    <li><a href="{{ linkedin_url }}" target="_blank"><i class="icofont-linkedin"></i></a></li>
  {% endif %}
  {% if youtube_url != '' %}
    <li><a href="{{ youtube_url }}" target="_blank"><i class="icofont-youtube-play"></i></a></li>
  {% endif %}
  {% if vimeo_url != '' %}
    <li><a href="{{ vimeo_url }}" target="_blank"><i class="icofont-vimeo"></i></a></li>
  {% endif %}
  {% if telegram_url != '' %}
    <li><a href="{{ telegram_url }}" target="_blank"><i class="icofont-telegram"></i></a></li>
  {% endif %}
  {% if whatsapp_url != '' %}
    <li><a href="{{ whatsapp_url }}" target="_blank"><i class="icofont-brand-whatsapp"></i></a></li>
  {% endif %}
  {% if github_url != '' %}
    <li><a href="{{ github_url }}" target="_blank"><i class="icofont-github"></i></a></li>
  {% endif %}
  {% if vk_url != '' %}
    <li><a href="{{ vk_url }}" target="_blank"><i class="icofont-vk" aria-hidden="true"></i></a></li>
  {% endif %}
</ul>
